<!DOCTYPE html>
<html>
<head>
  <title>Freemark</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  <div class="welcome">
    <% if current_user %>
      Welcome <%= current_user.email %>
    <% else %>
      <%= link_to "Sign In", new_user_session_url %>
    <% end %>
  </div>
<%= yield %>

</body>
</html>
